
import cloudinary
from cloudinary import uploader

# Set up Cloudinary configuration
cloudinary.config(
    cloud_name="your_cloud_name",
    api_key="your_api_key",
    api_secret="your_api_secret"
)

# Original video URL
original_video_url = "https://res.cloudinary.com/dk5acaaxg/video/upload/v1706589393/younova/videos/pskzmtjbolfwhyqsb27p.mp4"

# Remove the extension and add .jpg
new_image_url = original_video_url.rsplit('.', 1)[0] + ".jpg"

# Print the URL of the new image
print(f"Thumbnail URL: {new_image_url}")

